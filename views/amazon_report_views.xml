<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_amazon_duplicate_product_backend_tree" model="ir.ui.view">
        <field name="name">Amazon Duplicate Product on Backends</field>
        <field name="model">amazon.duplicate.product.backend.report</field>
        <field name="arch" type="xml">
            <tree string="Amazon Duplicate Product on Backends">
                <field name="sku"/>
                <field name="asin"/>
                <field name="backend_id"/>
            </tree>
        </field>
    </record>

    <record id="view_amazon_duplicate_product_backend_form" model="ir.ui.view">
        <field name="name">amazon.duplicate.product.backend.report.form</field>
        <field name="model">amazon.duplicate.product.backend.report</field>
        <field name="arch" type="xml">
            <form>
                <group><field name="sku"/></group>
                <group><field name="asin"/></group>
                <group><field name="backend_id"/></group>
            </form>
        </field>
    </record>

    <record id="view_amazon_duplicate_product_backend_graph" model="ir.ui.view">
            <field name="name">amazon.duplicate.product.backend.report.graph</field>
            <field name="model">amazon.duplicate.product.backend.report</field>
            <field name="arch" type="xml">
                <graph string="Amazon Duplicate Product on Backends" stacked="True">
                    <field name="sku" type="col"/>
                    <field name="asin" type="col"/>
                    <field name="backend_id" interval="month" type="row"/>
                </graph>
            </field>
        </record>

    <record id="action_amazon_duplicate_backend_products" model="ir.actions.act_window">
        <field name="name">Amazon Duplicate Backend Products</field>
        <field name="res_model">amazon.duplicate.product.backend.report</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form,graph</field>
        <field name="context">{}</field>
        <field name="domain">[]</field>
    </record>

</odoo>